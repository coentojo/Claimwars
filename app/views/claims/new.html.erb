Album Release Date:<%= @released %><br/>
Album Name:<%= @album %><br/>
Track title: <%= @track %><br/>
Artist: <%= @artist %><br/>

<%= form_for (@claim) do %>
	<%= hidden_field_tag :released, @released %>
	<%= hidden_field_tag :album, @album %>
	<%= hidden_field_tag :title, @track %>
	<%= hidden_field_tag :artist, @artist %>
	<%= hidden_field_tag :sid, @sid %>
	<%= submit_tag "CLAIM" %>
	
<% end %>
